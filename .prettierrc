{
    "tabWidth": 4,
    "useTabs": false,
    "endOfLine": "lf",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "printWidth": 90,
    "quoteProps": "as-needed",
    "trailingComma": "none",
    "semi": true,
    "singleQuote": true,
    "objectWrap": "preserve",
    "arrowParens": "avoid",
    "singleAttributePerLine": false
}
